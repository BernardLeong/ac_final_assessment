<h1>Create a new note here !</h1>

<%= form_for @note, url:notes_path, method: :post do |note|%>
  <%= current_user.email %>

  <h4>Title:</h4>
  <%= note.text_field :title ,placeholder: "Type your message gist here!" %>

  <h4>Body:</h4>
  <%= note.text_area :body ,placeholder: "Type your message body here!" %>
  <%= note.submit %>

<% end %>

